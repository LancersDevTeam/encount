{
    "name": "fusic/encount",
    "description": "Encount plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "symfony/var-dumper": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Encount\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Encount\\Test\\": "tests"
        }
    }
}
